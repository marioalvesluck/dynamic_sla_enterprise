{
	"manifest_version": 2,
	"id": "dynamic-sla-enterprise",
	"name": "Inteligence Enterprise",
	"version": "1.0",
	"namespace": "DynamicSlaEnterprise",
	"author": "Mario Alves",
	"url": "",
	"description": "Enterprise dynamic SLA analytics from incidents with multi filters.",
	"actions": {
		"dynamic.sla.enterprise.view": {
			"class": "CControllerDynamicSlaEnterpriseView",
			"view": "dynamic.sla.enterprise.view",
			"layout": "layout.htmlpage"
		},
		"dynamic.sla.enterprise.data": {
			"class": "CControllerDynamicSlaEnterpriseData"
		},
		"dynamic.sla.enterprise.report": {
			"class": "CControllerDynamicSlaEnterpriseReport",
			"view": "dynamic.sla.enterprise.report",
			"layout": "layout.htmlpage"
		}
	},
	"assets": {
		"css": [
			"blue-theme.css",
			"dark-theme.css",
			"report.css"
		],
		"js": []
	}
}
